    <!-- surveyData.ejs -->
<!-- comment-->
  <h1>Survey Data</h1>

  <!-- Dropdown for selecting distinct primary keys -->
  <label for="surveyKey">Select Survey Key:</label>
  <select id="surveyKey" name="surveyKey">
    <% distinctSurveyKeys.forEach(key => { %>
      <option value="<%= `${key.Timestamp}-${key.Age}-${key.Gender}` %>">
        <%= `${new Date(key.Timestamp).toLocaleString()}, ${key.Age}, ${key.Gender}` %>
      </option>
    <% }); %>
  </select>
  <!-- Table for displaying all survey data -->
  <table style="border: 1;">
    <thead>
      <tr>
        <th>Timestamp</th>
        <th>Age</th>
        <th>Gender</th>
        <th>Origin</th>
        <th>Relationship Status</th>
        <th>Occupation Status</th>
        <th>Social Media Use</th>
        <th>Mental Health Severity Score</th>
        <th>Social Media Score</th>
        <th>Average Time</th>
        <th>Social Media Type</th>
        <th>Organization Type</th>
        <th>SM Without Purpose</th>
        <th>SM Distracted</th>
        <th>SM Restless</th>
        <th>MH Distracted</th>
        <th>MH Worries</th>
        <th>MH Concentration</th>
        <th>SM Comparison</th>
        <th>SM Comparison Feeling</th>
        <th>SM Validation</th>
        <th>MH Depression</th>
        <th>MH Activity Interest</th>
        <th>MH Sleep</th>
        <!-- Add more headers as needed -->
      </tr>
    </thead>
    <tbody>
      <% surveyData.forEach(data => { %>
        <tr>
          <td><%= new Date(data.Timestamp).toLocaleString() %></td>
          <td><%= data.Age %></td>
          <td><%= data.Gender %></td>
          <td><%= data.Origin %></td>
          <td><%= data.RelationshipStatus %></td>
          <td><%= data.OccupationStatus %></td>
          <td><%= data.SocialMediaUse %></td>
          <td><%= data.MentalHealthSeverityScore %></td>
          <td><%= data.SocialMediaScore %></td>
          <td><%= data.AverageTime %></td>
          <td><%= data.SocialMediaType %></td>
          <td><%= data.OrgType %></td>
          <td><%= data.SM_WithoutPurpose %></td>
          <td><%= data.SM_Distracted %></td>
          <td><%= data.SM_Restless %></td>
          <td><%= data.MH_Distracted %></td>
          <td><%= data.MH_Worries %></td>
          <td><%= data.MH_Concentration %></td>
          <td><%= data.SM_Comparison %></td>
          <td><%= data.SM_ComparisonFeeling %></td>
          <td><%= data.SM_Validation %></td>
          <td><%= data.MH_Depression %></td>
          <td><%= data.MH_ActivityInterest %></td>
          <td><%= data.MH_Sleep %></td>
          <!-- Add more columns as needed -->
        </tr>
      <% }); %>
    </tbody>
  </table>  
    <!-- ======= Footer ======= -->
    <footer id="footer">
      <div class="footer-top">
        <div class="container">
          <div class="row">
  
            <div class="col-lg-4 col-md-6">
              <div class="footer-info">
                <h3>Provo Mental Health Organization</h3>
                <p>
                  445 W Center St <br>
                  UT 84601, USA<br><br>
                  <strong>Phone:</strong> +1 (801) 852-6120<br>
                </p>
                <div class="social-links mt-3">
                  <a href="https://twitter.com/UCHD" class="twitter"><i class="bx bxl-twitter"></i></a>
                  <a href="https://www.facebook.com/uchealth/" class="facebook"><i class="bx bxl-facebook"></i></a>
                  <a href="https://www.instagram.com/provohealth/?hl=en" class="instagram"><i class="bx bxl-instagram"></i></a>
                  <a href="https://www.linkedin.com/in/provo-canyon-behavioral-hospital-a49344141/" class="linkedin"><i class="bx bxl-linkedin"></i></a>
                </div>
              </div>
            </div>
  
            <div class="col-lg-4 col-md-6 footer-newsletter">
              <h4>Enter Email below to download our highly recommended mental health app!</h4>
              <form action="https://www.sanvello.com/" method="get">
                <input type="email" name="email"><input type="submit" value="Subscribe">
              </form>
  
            </div>
  
          </div>
        </div>
      </div>
  
      <div class="container">
        <div class="copyright">
          &copy; Copyright <strong><span>Provo Mental Health Organization</span></strong>. All Rights Reserved
        </div>
      </div>
    </footer>
  
    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>
